# Search in full text for thesaurus term
# Datasource: https://cat.apis.beeldengeluid.nl/sparql
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX sdo: <https://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX gtaa:<http://data.beeldengeluid.nl/gtaa/>

SELECT DISTINCT ?s ?pref_label ?concept_scheme
WHERE {
    VALUES ?query { "oorlog" }
  ?s (skos:prefLabel|skos:altLabel|skos:hiddenLabel) ?label .
  ?s skos:prefLabel ?pref_label .
    ?s skos:inScheme ?concept_scheme
  FILTER regex(?label, ?query, "i" )
}
